        "type":"select",
        "width":3,
        "height":5,
        "options":[
          "2022",
          "2023", 
          "2024"
        ],
        "value": "2024",
        "multiple": false,
        "placeholder":"Select year",
        "required":true,
        "disabled":false
      },
      {
        "label": "Time",
        "type":"multiselect",
        "width":3,
        "height":5,
        "options":[
          "11",
          "12",
          "13",
